---
import Layout from "../layouts/Layout.astro";

const list = [["random_thing", "https://randomthing.isextraordinary.com"]];
---

<Layout>
  <div class="pl-4">
    <ul>
      {
        list.map(([title, url]) => (
          <li class="hover:text-gray-400">
            <a href={url}> &gt; {title} </a>
          </li>
        ))
      }
    </ul>
  </div>
</Layout>
